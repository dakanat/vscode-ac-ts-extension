{
  "version": "0.0.63",
  "name": "ac-ts-extension",
  "displayName": "AtCoder/Yukicoder Extension",
  "description": "atcoder/yukicoder vscode extension for python/typescript/javascript",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "atcoder",
    "yukicoder",
    "python",
    "typescript",
    "javascript",
    "windows"
  ],
  "publisher": "taizod1024",
  "main": "./build/main.js",
  "activationEvents": [
    "onCommand:ac-ts-extension.loginSite",
    "onCommand:ac-ts-extension.initTask",
    "onCommand:ac-ts-extension.reinitTask",
    "onCommand:ac-ts-extension.testTask",
    "onCommand:ac-ts-extension.debugTask",
    "onCommand:ac-ts-extension.submitTask",
    "onCommand:ac-ts-extension.removeTask",
    "onCommand:ac-ts-extension.browseTask"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ac-ts-extension.loginSite",
        "title": "AtCoder/Yukicoder Extension: login Site..."
      },
      {
        "command": "ac-ts-extension.initTask",
        "title": "AtCoder/Yukicoder Extension: Init Task..."
      },
      {
        "command": "ac-ts-extension.reinitTask",
        "title": "AtCoder/Yukicoder Extension: Reinit Task..."
      },
      {
        "command": "ac-ts-extension.testTask",
        "title": "AtCoder/Yukicoder Extension: Test Task"
      },
      {
        "command": "ac-ts-extension.debugTask",
        "title": "AtCoder/Yukicoder Extension: Debug Task"
      },
      {
        "command": "ac-ts-extension.submitTask",
        "title": "AtCoder/Yukicoder Extension: Submit Task"
      },
      {
        "command": "ac-ts-extension.removeTask",
        "title": "AtCoder/Yukicoder Extension: Remove Task..."
      },
      {
        "command": "ac-ts-extension.browseTask",
        "title": "AtCoder/Yukicoder Extension: Browse Task"
      }
    ]
  },
  "scripts": {
    "nodecheck": "node --version | findstr \"v16.13.0\"",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=build/main.js --format=cjs --platform=node --external:vscode --external:vm2",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test": "node ./build/test/runTest.js",
    "release": "npm run release-premerge && npx standard-version && vsce publish && npm run release-postmerge",
    "release-premerge": "npm run nodecheck && git switch main && git merge --no-ff develop",
    "release-postmerge": "git push --follow-tags origin main && git push origin develop && git switch develop",
    "package": "npm run nodecheck && vsce package",
    "clean": "rmdir /q /s .\\build || exit /b 0"
  },
  "author": "yamamoto.taizo <taizod1024@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "superagent": "^6.1.0",
    "superagent-proxy": "^3.0.0",
    "vm2": "^3.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.4",
    "@types/superagent": "^4.1.13",
    "@types/superagent-proxy": "^2.0.1",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "bufferutil": "^4.0.3",
    "esbuild": "^0.13.6",
    "eslint": "^7.32.0",
    "generator-code": "^1.6.1",
    "glob": "^7.2.0",
    "husky": "^4.3.8",
    "mocha": "^9.1.1",
    "standard-version": "^9.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "utf-8-validate": "^5.0.5",
    "vscode-test": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taizod1024/ac-ts-extension.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./bin/check_no_main_branch.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
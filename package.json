{
  "version": "0.0.70",
  "name": "ac-ts-extension",
  "displayName": "AtCoder/Yukicoder Extension",
  "description": "atcoder/yukicoder vscode extension for python/typescript",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "taizod1024",
  "main": "./build/main.js",
  "activationEvents": [
    "onCommand:ac-ts-extension.loginSite",
    "onCommand:ac-ts-extension.initTask",
    "onCommand:ac-ts-extension.reinitTask",
    "onCommand:ac-ts-extension.testTask",
    "onCommand:ac-ts-extension.debugTask",
    "onCommand:ac-ts-extension.submitTask",
    "onCommand:ac-ts-extension.removeTask",
    "onCommand:ac-ts-extension.browseTask"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ac-ts-extension.loginSite",
        "title": "AtCoder Extension: login Site..."
      },
      {
        "command": "ac-ts-extension.initTask",
        "title": "AtCoder Extension: Init Task..."
      },
      {
        "command": "ac-ts-extension.reinitTask",
        "title": "AtCoder Extension: Reinit Task..."
      },
      {
        "command": "ac-ts-extension.testTask",
        "title": "AtCoder Extension: Test Task"
      },
      {
        "command": "ac-ts-extension.debugTask",
        "title": "AtCoder Extension: Debug Task"
      },
      {
        "command": "ac-ts-extension.submitTask",
        "title": "AtCoder Extension: Submit Task"
      },
      {
        "command": "ac-ts-extension.removeTask",
        "title": "AtCoder Extension: Remove Task..."
      },
      {
        "command": "ac-ts-extension.browseTask",
        "title": "AtCoder Extension: Browse Task"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=build/main.js --format=cjs --platform=node --external:vscode --external:vm2",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test": "node ./build/test/runTest.js",
    "release": "npm run release-step1 && npm run release-step2",
    "release-step1": "npm run clean && git switch main && git merge --no-ff develop",
    "release-step2": "npx standard-version && vsce publish && git push --follow-tags origin main && git push origin develop && git switch develop",
    "package": "vsce package",
    "clean": "rmdir /q /s .\\build"
  },
  "author": "yamamoto.taizo <taizod1024@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "superagent": "^6.1.0",
    "superagent-proxy": "^3.0.0",
    "vm2": "^3.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "@types/superagent": "^4.1.13",
    "@types/superagent-proxy": "^2.0.1",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "bufferutil": "^4.0.3",
    "esbuild": "^0.13.6",
    "eslint": "^7.32.0",
    "generator-code": "^1.6.1",
    "glob": "^7.2.0",
    "husky": "^4.3.8",
    "mocha": "^9.1.1",
    "standard-version": "^9.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "utf-8-validate": "^5.0.5",
    "vscode-test": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taizod1024/ac-ts-extension.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./bin/check_no_main_branch.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}

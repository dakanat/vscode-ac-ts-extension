{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/naming-convention": "off",
    "@typescript-eslint/semi": "warn",
    "curly": "off",
    "eqeqeq": "warn",
    "no-throw-literal": "off",
    "no-unused-vars": "warn",
    "semi": "warn"
  },
  "ignorePatterns": [
    "**/*.d.ts"
  ]
}